<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.baohong.poop">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_round"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".FirstPage">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <activity android:windowSoftInputMode="adjustPan" android:name=".CreateName" />
        <activity android:windowSoftInputMode="adjustPan" android:name=".CreateContactInfo" />
        <activity android:windowSoftInputMode="adjustPan" android:name=".CreateUserPass" />
        <activity android:windowSoftInputMode="adjustPan" android:name=".CreateVenAddress" />
        <activity android:windowSoftInputMode="adjustPan" android:name=".CreateVenOrFT" />
        <activity android:windowSoftInputMode="adjustPan" android:name=".CreateFTLocation" />
        <activity android:windowSoftInputMode="adjustPan" android:name=".MainContentPg" />
        <activity android:windowSoftInputMode="adjustPan" android:name=".Login"></activity>
    </application>

</manifest>